{
  "name": "@ciqada/ui-hero",
  "version": "1.0.0",
  "description": "CLI tool to install UI Hero landing sections",
  "main": "dist/index.js",
  "bin": {
    "ui-hero": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --clean --external @clack/prompts --external chalk --external ora --external commander",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ui",
    "components",
    "nextjs",
    "landing-page",
    "tailwind"
  ],
  "author": "UI Hero",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
